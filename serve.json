{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "http://backend:3001/$1"
    }
  ],
  "headers": [
    {
      "source": "**/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}